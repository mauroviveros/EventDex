---
import AuthStatus from "./AuthStatus.astro";
import Brand from "./Brand.astro";
interface Props { loggedin?: boolean; }
const { loggedin = false } = Astro.props;
---

<header class="flex items-center border-b h-16 w-full justify-between">
  <Brand/>
  <AuthStatus loggedin={loggedin}/>
</header>
